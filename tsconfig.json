{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "./dst"
  },
   "include": ["src/**/*"]
}
